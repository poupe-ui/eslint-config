{
  "name": "@poupe/eslint-config",
  "version": "0.4.19",
  "type": "module",
  "description": "",
  "author": "Alejandro Mery <amery@apptly.co>",
  "license": "MIT",
  "homepage": "https://github.com/poupe-ui/eslint-config",
  "repository": "github:poupe-ui/eslint-config",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./nuxt": {
      "import": "./dist/nuxt.mjs",
      "require": "./dist/nuxt.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "pnpm build",
    "dev:prepare": "unbuild --stub",
    "postinstall": "[ -s dist/index.mjs ] || unbuild --stub",
    "lint": "unbuild --stub && env DEBUG=eslint:eslint eslint --fix .",
    "build": "unbuild",
    "clean": "rm -rf dist node_modules",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@eslint/js": "^9.20.0",
    "@stylistic/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^8.24.1",
    "@vue/eslint-config-typescript": "^14.4.0",
    "eslint": "^9.20.1",
    "eslint-plugin-tsdoc": "^0.4.0",
    "eslint-plugin-unicorn": "^57.0.0",
    "eslint-plugin-vue": "^9.32.0",
    "typescript-eslint": "^8.24.1"
  },
  "devDependencies": {
    "pkg-pr-new": "^0.0.40",
    "typescript": "~5.6.3",
    "unbuild": "3.3.1"
  },
  "engines": {
    "node": ">= 18.20.6",
    "pnpm": ">= 10.4.1"
  },
  "packageManager": "pnpm@10.4.1+sha512.c753b6c3ad7afa13af388fa6d808035a008e30ea9993f58c6663e2bc5ff21679aa834db094987129aa4d488b86df57f7b634981b2f827cdcacc698cc0cfb88af"
}

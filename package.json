{
  "name": "@poupe/eslint-config",
  "version": "0.4.12",
  "type": "module",
  "description": "",
  "homepage": "https://github.com/poupe-ui/eslint-config",
  "repository": "github:poupe-ui/eslint-config",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./nuxt": {
      "import": "./dist/nuxt.mjs",
      "require": "./dist/nuxt.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "pnpm build",
    "dev:prepare": "unbuild --stub",
    "postinstall": "[ -s dist/index.mjs ] || unbuild --stub",
    "lint": "unbuild --stub && env DEBUG=eslint:eslint eslint --fix .",
    "build": "unbuild",
    "clean": "rm -rf dist node_modules",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@eslint/js": "^9.18.0",
    "@stylistic/eslint-plugin": "^2.12.1",
    "@typescript-eslint/parser": "^8.20.0",
    "@vue/eslint-config-typescript": "^14.2.0",
    "eslint": "^9.18.0",
    "eslint-plugin-tsdoc": "^0.4.0",
    "eslint-plugin-unicorn": "^56.0.1",
    "eslint-plugin-vue": "^9.32.0",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "devDependencies": {
    "unbuild": "3.2.0"
  },
  "engines": {
    "node": ">= 18.20.5",
    "pnpm": ">= 9.15.3"
  },
  "packageManager": "pnpm@9.15.3+sha512.1f79bc245a66eb0b07c5d4d83131240774642caaa86ef7d0434ab47c0d16f66b04e21e0c086eb61e62c77efc4d7f7ec071afad3796af64892fae66509173893a"
}

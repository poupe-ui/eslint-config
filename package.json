{
  "name": "@poupe/eslint-config",
  "version": "0.4.13",
  "type": "module",
  "description": "",
  "homepage": "https://github.com/poupe-ui/eslint-config",
  "repository": "github:poupe-ui/eslint-config",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./nuxt": {
      "import": "./dist/nuxt.mjs",
      "require": "./dist/nuxt.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "pnpm build",
    "dev:prepare": "unbuild --stub",
    "postinstall": "[ -s dist/index.mjs ] || unbuild --stub",
    "lint": "unbuild --stub && env DEBUG=eslint:eslint eslint --fix .",
    "build": "unbuild",
    "clean": "rm -rf dist node_modules",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@eslint/js": "^9.18.0",
    "@stylistic/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^8.20.0",
    "@vue/eslint-config-typescript": "^14.3.0",
    "eslint": "^9.18.0",
    "eslint-plugin-tsdoc": "^0.4.0",
    "eslint-plugin-unicorn": "^56.0.1",
    "eslint-plugin-vue": "^9.32.0",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "devDependencies": {
    "unbuild": "3.3.1"
  },
  "engines": {
    "node": ">= 18.20.5",
    "pnpm": ">= 9.15.4"
  },
  "packageManager": "pnpm@9.15.4+sha512.b2dc20e2fc72b3e18848459b37359a32064663e5627a51e4c74b2c29dd8e8e0491483c3abb40789cfd578bf362fb6ba8261b05f0387d76792ed6e23ea3b1b6a0"
}

{
  "name": "@poupe/eslint-config",
  "version": "0.4.16",
  "type": "module",
  "description": "",
  "author": "Alejandro Mery <amery@apptly.co>",
  "license": "MIT",
  "homepage": "https://github.com/poupe-ui/eslint-config",
  "repository": "github:poupe-ui/eslint-config",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./nuxt": {
      "import": "./dist/nuxt.mjs",
      "require": "./dist/nuxt.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "pnpm build",
    "dev:prepare": "unbuild --stub",
    "postinstall": "[ -s dist/index.mjs ] || unbuild --stub",
    "lint": "unbuild --stub && env DEBUG=eslint:eslint eslint --fix .",
    "build": "unbuild",
    "clean": "rm -rf dist node_modules",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@eslint/js": "^9.20.0",
    "@stylistic/eslint-plugin": "^3.0.1",
    "@typescript-eslint/parser": "^8.23.0",
    "@vue/eslint-config-typescript": "^14.3.0",
    "eslint": "^9.20.0",
    "eslint-plugin-tsdoc": "^0.4.0",
    "eslint-plugin-unicorn": "^56.0.1",
    "eslint-plugin-vue": "^9.32.0",
    "typescript-eslint": "^8.23.0"
  },
  "devDependencies": {
    "pkg-pr-new": "^0.0.39",
    "typescript": "~5.6.3",
    "unbuild": "3.3.1"
  },
  "engines": {
    "node": ">= 18.20.6",
    "pnpm": ">= 10.2.1"
  },
  "packageManager": "pnpm@10.2.1+sha512.398035c7bd696d0ba0b10a688ed558285329d27ea994804a52bad9167d8e3a72bcb993f9699585d3ca25779ac64949ef422757a6c31102c12ab932e5cbe5cc92"
}

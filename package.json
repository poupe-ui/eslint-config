{
  "name": "@poupe/eslint-config",
  "version": "0.6.1",
  "type": "module",
  "description": "",
  "author": "Alejandro Mery <amery@apptly.co>",
  "license": "MIT",
  "homepage": "https://github.com/poupe-ui/eslint-config",
  "repository": "github:poupe-ui/eslint-config",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./nuxt": {
      "import": "./dist/nuxt.mjs",
      "require": "./dist/nuxt.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "pnpm build",
    "dev:prepare": "unbuild --stub",
    "postinstall": "[ -s dist/index.mjs ] || unbuild --stub",
    "lint": "unbuild --stub && env DEBUG=eslint:eslint eslint --fix .",
    "build": "unbuild",
    "clean": "rm -rf dist node_modules",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@eslint/js": "^9.25.1",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^8.31.1",
    "@vue/eslint-config-typescript": "^14.5.0",
    "eslint": "^9.25.1",
    "eslint-plugin-tsdoc": "^0.4.0",
    "eslint-plugin-unicorn": "^59.0.0",
    "eslint-plugin-vue": "^10.1.0",
    "typescript-eslint": "^8.31.1"
  },
  "devDependencies": {
    "pkg-pr-new": "^0.0.43",
    "typescript": "~5.8.3",
    "unbuild": "3.5.0"
  },
  "engines": {
    "node": ">= 18.20.8",
    "pnpm": ">= 10.10.0"
  },
  "packageManager": "pnpm@10.10.0+sha512.d615db246fe70f25dcfea6d8d73dee782ce23e2245e3c4f6f888249fb568149318637dca73c2c5c8ef2a4ca0d5657fb9567188bfab47f566d1ee6ce987815c39"
}
